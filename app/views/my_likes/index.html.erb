<div class="page-header">
  <h1>
    My Likes
    <a href="/likes/new" class="btn btn-lg btn-success">New Like</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Photo</th>
        <th>Caption</th>

      </tr>

      <% @my_likes.each do |mylike| %>
      <tr>
        <td><img src="<%= mylike.photo.image %>" width="200"></td>


        <td><%= mylike.photo.caption %></td>



      </tr>

      <% end %>
    </table>
  </div>
</div>
